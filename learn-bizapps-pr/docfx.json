{
  "build": {
    "content": [
      {
        "files": [
          "achievements.yml"
        ],
        "dest": "achievements/bizapps/"
      },
      {
        "src": "dyn365-customer-service",
        "files": [
          "**.yml"
        ],
        "dest": "modules"
      },
      {
        "src": "powerapps",
        "files": [
          "**.yml"
        ],
        "dest": "modules"
      },
      {
        "src": "power-bi",
        "files": [
          "**.yml"
        ],
        "dest": "modules"
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "paths/sample-path/**"
        ]
      }
    ],
    "resource": [
      {
        "src": "dyn365-customer-service",
        "files": [
          "**/*.gif",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.jpeg"
        ],
        "dest": "modules"
      },
      {
        "src": "powerapps",
        "files": [
          "**/*.gif",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.jpeg"
        ],
        "dest": "modules"
      },
      {
        "src": "power-bi",
        "files": [
          "**/*.gif",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.jpeg"
        ],
        "dest": "modules"
      },
      {
        "files": [
          "**/*.gif",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.jpeg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "paths/sample-path/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/learn/breadcrumb/toc.json",
